  object qryvaga: TQuery
    DatabaseName = 'TalentPool'
    SQL.Strings = (
      'select'
      ' v.Vag_cod Vaga,'
      ' e.emp_nome [Empresa],'
      
        '(select  sel_nome from  selecao  where v.sel_cod = sel_cod)  Ges' +
        'tor,'
      'case'
      ' when vag_sexo = 0 then '#39'Masculino'#39
      ' when vag_sexo = 1 then '#39'Feminino'#39
      ' when vag_sexo = 2 then '#39'Ambos'#39
      'end Sexo,'
      'c.cur_nome Curso,'
      
        #39'Do '#39' + convert(char(1), vc.vcu_periodoini) + '#39#186' ao '#39' + convert(' +
        'char(1),vc.vcu_periodofim)+'#39#186#39', '
      'v.vag_reqdivulgacao [Requisitos para Divulga'#231#227'o],'
      
        'v.vag_horaprocesso [Hor'#225'rio, Bolsa Aux'#237'lio e Benef'#237'cios  (para d' +
        'ivulga'#231#227'o de vagas)],'
      'v.vag_dataprocesso [Data e hor'#225'rio da Coletiva],'
      'e.emp_bairro [Bairro],'
      'v.Vag_reqinterno [Requisitos],'
      'V.vag_observacao [Observa'#231#245'es],'
      'v.vag_dataabertura [Data Abertura],'
      'v.vag_qtde [Qtde.],'
      
        '(select count(vag_cod) from encaminhamento where vag_cod = v.vag' +
        '_cod and eva_resultado ='#39'4'#39') [Selecionado Pela Empresa],'
      
        '(select count(vag_cod) from encaminhamento where vag_cod = v.vag' +
        '_cod and eva_resultado ='#39'2'#39') [Aprovado],'
      
        '(select count(vag_cod) from encaminhamento where vag_cod = v.vag' +
        '_cod and eva_resultado ='#39'0'#39') [Em Espera],'
      ''
      
        '(select count(vag_cod) from encaminhamento where vag_cod = v.vag' +
        '_cod and eva_resultado ='#39'1'#39') [Reprovado],'
      
        '(select count(vag_cod) from encaminhamento where vag_cod = v.vag' +
        '_cod and eva_resultado ='#39'6'#39') [Stand By],'
      
        '(select count(vag_cod) from encaminhamento where vag_cod = v.vag' +
        '_cod and eva_resultado ='#39'5'#39') [N'#227'o entrevistado pelo cliente],'
      
        '(select count(vag_cod) from encaminhamento where vag_cod = v.vag' +
        '_cod and eva_resultado ='#39'3'#39') [Desistiu],'
      #39'formula1'#39' [Necessidade de Encaminhamento],'
      #39'formula2'#39' [Vagas Pendentes],'
      'case '
      'when vag_acao = '#39'R'#39' then '#39'Falta Retorno do Cliente '#39
      'when vag_acao = '#39'P'#39' then '#39'Em Processo Seletivo '#39
      'when vag_acao = '#39'T'#39' then '#39'Falta Emiss'#227'o de TCE'#39
      'when vag_acao = '#39'F'#39' then '#39'Vaga Fechada'#39
      'when vag_acao = '#39'S'#39' then '#39'Sem Analise'#39
      'when vag_acao = '#39'V'#39' then '#39'Vaga Permanente'#39
      'end   [Acao],'
      #39'formula3'#39' [N'#186' de TCEs para Emiss'#227'o]'
      'from'
      '  Vaga V, Empresa E, VagaCurso vC,Curso C'
      'where '
      'E.emp_cod = V.emp_cod '
      'and vC.Vag_cod = V.vag_cod '
      'and C.cur_cod=vc.cur_cod'
      
        'and v.vag_qtdepreenc + coalesce(v.vag_qtdecancel,0) < vag_qtde a' +
        'nd vag_cancelada <> '#39'1'#39' '
      'order by   v.vag_cod')
    Left = 576
    Top = 336
