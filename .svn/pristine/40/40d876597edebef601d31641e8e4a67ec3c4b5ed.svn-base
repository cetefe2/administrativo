  object Query1: TQuery
    DatabaseName = 'TalentPool'
    SQL.Strings = (
      'select  agenda.age_data, '
      
        '     (select est_nome from  Estudante where Agenda.age_estcod1 =' +
        ' est_cod ) Estudante,  '
      
        '     (select emp_nome from  Empresa where  Emp_cod in (select Em' +
        'p_cod from vaga where vag_cod = Agenda.age_vagcod1)) Empresa,  '
      
        '     (select vag_setor from vaga where vag_cod = Agenda.age_vagc' +
        'od1) Area,  '
      
        '     (select eva_dataenc from  Encaminhamento where Agenda.age_e' +
        'stcod1 = est_cod and Agenda.age_vagcod1 = vag_cod ) Encaminhamen' +
        'to,   '
      '     case      '
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod1 = est_cod and Agenda.age_vagcod1 = vag_cod ) = ' +
        '0  then '#39'Em Espera'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod1 = est_cod and Agenda.age_vagcod1 = vag_cod ) = ' +
        '1  then '#39'Reprovado'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod1 = est_cod and Agenda.age_vagcod1 = vag_cod ) = ' +
        '2  then '#39'Aprovado'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod1 = est_cod and Agenda.age_vagcod1 = vag_cod ) = ' +
        '3  then '#39'Desistiu'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod1 = est_cod and Agenda.age_vagcod1 = vag_cod ) = ' +
        '4  then '#39'Selecionado pela Empresa'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod1 = est_cod and Agenda.age_vagcod1 = vag_cod ) = ' +
        '5  then '#39'N'#227'o Entrevistado pelo Cliente'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod1 = est_cod and Agenda.age_vagcod1 = vag_cod ) = ' +
        '6  then '#39'STANDBY'#39
      '       else'
      '       '#39'N'#227'o encaminhado'#39
      '      end Resultado,'
      ''
      '      age_faltou1 Compareceu ,age_dtenvio1 Aviso_de_Falta'
      '     from Agenda  where age_estcod1 is not null '
      'union   '
      'select  agenda.age_data, '
      
        '     (select est_nome from  Estudante where Agenda.age_estcod2 =' +
        ' est_cod ) Estudante,  '
      
        '    (select emp_nome from  Empresa where  Emp_cod in (select Emp' +
        '_cod from vaga where vag_cod = Agenda.age_vagcod2)) Empresa,  '
      
        '     (select vag_setor from vaga where vag_cod = Agenda.age_vagc' +
        'od2) Area,  '
      
        '     (select eva_dataenc from  Encaminhamento where Agenda.age_e' +
        'stcod2 = est_cod and Agenda.age_vagcod2 = vag_cod ) Encaminhamen' +
        'to,   '
      '          case      '
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod2 = est_cod and Agenda.age_vagcod2 = vag_cod ) = ' +
        '0  then '#39'Em Espera'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod2 = est_cod and Agenda.age_vagcod2 = vag_cod ) = ' +
        '1  then '#39'Reprovado'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod2 = est_cod and Agenda.age_vagcod2 = vag_cod ) = ' +
        '2  then '#39'Aprovado'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod2 = est_cod and Agenda.age_vagcod2 = vag_cod ) = ' +
        '3  then '#39'Desistiu'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod2 = est_cod and Agenda.age_vagcod2 = vag_cod ) = ' +
        '4  then '#39'Selecionado pela Empresa'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod2 = est_cod and Agenda.age_vagcod2 = vag_cod ) = ' +
        '5  then '#39'N'#227'o Entrevistado pelo Cliente'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod2 = est_cod and Agenda.age_vagcod2 = vag_cod ) = ' +
        '6  then '#39'STANDBY'#39
      '       else'
      '       '#39'N'#227'o encaminhado'#39
      '      end Resultado,'
      '      age_faltou2 Compareceu ,age_dtenvio2 Aviso_de_Falta'
      '     from Agenda  where age_estcod2 is not null '
      'union'
      'select  agenda.age_data, '
      
        '     (select est_nome from  Estudante where Agenda.age_estcod3 =' +
        ' est_cod ) Estudante,  '
      
        '     (select emp_nome from  Empresa where  Emp_cod in (select Em' +
        'p_cod from vaga where vag_cod = Agenda.age_vagcod3)) Empresa,  '
      
        '     (select vag_setor from vaga where vag_cod = Agenda.age_vagc' +
        'od3) Area,  '
      
        '     (select eva_dataenc from  Encaminhamento where Agenda.age_e' +
        'stcod3 = est_cod and Agenda.age_vagcod3 = vag_cod ) Encaminhamen' +
        'to,   '
      '          case      '
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod3 = est_cod and Agenda.age_vagcod3 = vag_cod ) = ' +
        '0  then '#39'Em Espera'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod3 = est_cod and Agenda.age_vagcod3 = vag_cod ) = ' +
        '1  then '#39'Reprovado'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod3 = est_cod and Agenda.age_vagcod3 = vag_cod ) = ' +
        '2  then '#39'Aprovado'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod3 = est_cod and Agenda.age_vagcod3 = vag_cod ) = ' +
        '3  then '#39'Desistiu'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod3 = est_cod and Agenda.age_vagcod3 = vag_cod ) = ' +
        '4  then '#39'Selecionado pela Empresa'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod3 = est_cod and Agenda.age_vagcod3 = vag_cod ) = ' +
        '5  then '#39'N'#227'o Entrevistado pelo Cliente'#39
      
        '       when  (select eva_resultado from  Encaminhamento where Ag' +
        'enda.age_estcod3 = est_cod and Agenda.age_vagcod3 = vag_cod ) = ' +
        '6  then '#39'STANDBY'#39
      '       else'
      '      '#39'N'#227'o encaminhado'#39
      '      end Resultado,'
      '      age_faltou3 Compareceu ,age_dtenvio3 Aviso_de_Falta'
      '     from Agenda   where age_estcod3 is not null '
      ''
      'order by age_data')
    Left = 448
    Top = 232
