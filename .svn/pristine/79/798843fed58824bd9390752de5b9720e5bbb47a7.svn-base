    with TQuery.Create(nil) do
    begin
      DatabaseName := DATABASE_NAME;
      SQL.Clear;
      SQL.Add('select');
      SQL.Add('  max(lot_sequencia) as Maximo,');
      SQL.Add('  min(lot_sequencia) as Minimo');
      SQL.Add('from');
      SQL.Add('  lote');
      Open;
